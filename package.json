    "@bytemd/plugin-medium-zoom": "^1.17.2",
    "@bytemd/plugin-mermaid": "^1.17.2",
    "@bytemd/react": "^1.17.2",
    "@reduxjs/toolkit": "^1.8.4",
    "ahooks": "^3.7.0",
    "antd": "^4.22.6",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "echarts-for-react": "^3.0.2",
    "markdown-navbar": "^1.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-top-loading-bar": "^2.1.0",
    "recoil": "^0.7.5",
